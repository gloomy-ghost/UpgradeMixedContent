{
	"manifest_version": 2,
	"name": "MixedContentHunter",
	"version": "0.2.2",
	"description": "Resloves some mixedcontent blocking.",
	"icons": {
		"48": "icons/icon.png"
	},
	"permissions": [
		"https://*/*", "webRequest", "webRequestBlocking"
	],
	"background": {
		"scripts": ["background.js"]
	},
	"options_ui": {
		"page": "options/options.html"
	}
}
